# Next.js static export SPA rewrite rules

<IfModule mod_rewrite.c>
  RewriteEngine On
  # prevent Apache from serving up MultiViews items
  Options -MultiViews

  # if the request is not for an existing file or directory,
  # rewrite to index.html so the client router can handle it
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule ^ /index.html [L]
</IfModule>
